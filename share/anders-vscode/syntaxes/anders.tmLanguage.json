{
    "name": "anders",
    "fileTypes": [
        "anders"
    ],
    "patterns": [
        {
            "begin": "{-",
            "end": "-}",
            "name": "comment.block.anders"
        },
        {
            "match": "--.*$",
            "name": "comment.line.double-dash.anders"
        },
        {
            "match": "(?<=^|[\\s()])(U|option|axiom|postulate|def|definition|theorem|lemma|corollary|proposition|hdata|data|import|mutual|let|in|split|with|module|where|opaque|transparent|transparent_all)(?=$|[\\s()])",
            "name": "keyword.other.anders"
        },
        {
            "match": "(?<=^|[\\s()])(λ|Σ|Π|sigma|pi|PathP|LineP|V|Vin|Vproj|comp|hcomp|hcom|com|coe|transport|fill|Glue|glue|unglue|Id|idC|idJ)(?=$|[\\s()])",
            "name": "keyword.control.anders"
        },
        {
            "match": "(?<=^|[\\s()])(->|→|:=|≔|≜|≝|:|\\*|\\\\/|/\\\\|∧|∨|@)(?=$|[\\s()])",
            "name": "keyword.other.anders"
        },
        {
            "match": "(?<=^|[\\s()])(-)",
            "name": "keyword.other.anders"
        },
        {
            "match": "[><]",
            "name": "keyword.other.anders"
        }
    ],
    "scopeName": "source.anders"
}
