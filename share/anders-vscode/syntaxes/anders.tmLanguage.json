{
    "name": "anders",
    "fileTypes": [
        "anders"
    ],
    "patterns": [
        {
            "begin": "{-",
            "end": "-}",
            "name": "comment.block.anders"
        },
        {
            "match": "--.*$",
            "name": "comment.line.double-dash.anders"
        },
        {
            "match": "(?<=^|[\\s()])(option|axiom|postulate|def|definition|theorem|lemma|corollary|proposition|hdata|data|import|mutual|let|in|split|with|module|where|opaque|transparent|transparent_all)(?=$|[\\s()])",
            "name": "keyword.other.anders"
        },
        {
            "match": "(?<=^|[\\s(),])(λ|Σ|Π|Id|ref|idJ|forall|sigma|Partial|inc|ouc|PathP|hcomp|transp|Glue|glue|unglue)(?=$|[\\s(),])",
            "name": "keyword.control.anders"
        },
        {
            "match": "(?<=^|[\\s(),])([UV][₀₁₂₃₄₅₆₇₈₉]*)(?=$|[\\s(),])",
            "name": "keyword.other.anders"
        },
        {
            "match": "(?<=^|[\\s()])(->|→|↦|\\|->|:=|≔|≜|≝|:|\\*|\\\\/|/\\\\|∧|∨|@)(?=$|[\\s()])",
            "name": "keyword.other.anders"
        },
        {
            "match": "(?<=^|[\\s()])(-)",
            "name": "keyword.other.anders"
        },
        {
            "match": "[><]",
            "name": "keyword.other.anders"
        }
    ],
    "scopeName": "source.anders"
}
